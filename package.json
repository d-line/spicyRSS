{
  "name": "spicyrss",
  "version": "0.0.1",
  "description": "RSS Reader",
  "main": "dist/server.js",
  "scripts": {
    "prebuild": "npm run lint",
    "build": "tsc",
    "prestart": "npm run build",
    "start": "node .",
    "lint:fix": "eslint --cache --ext .ts . --fix",
    "lint": "eslint --cache --ext .ts .",
    "server:watch": "ts-node-dev --respawn --transpile-only src/server.ts",
    "dev": "ts-node ./src/server.ts",
    "test": "npm run test:unit",
    "test:unit": "jest --config=jest.config.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/d-line/spicyRSS.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/d-line/spicyRSS/issues"
  },
  "homepage": "https://github.com/d-line/spicyRSS#readme",
  "dependencies": {
    "bcrypt": "^5.0.0",
    "body-parser": "^1.19.0",
    "class-transformer": "^0.3.2",
    "class-validator": "^0.13.1",
    "cookie-parser": "^1.4.5",
    "dotenv": "^8.2.0",
    "envalid": "^6.0.2",
    "express": "^4.17.1",
    "jsonwebtoken": "^8.5.1",
    "log4js": "^6.3.0",
    "mongoose": "^5.10.3",
    "multer": "^1.4.2",
    "reflect-metadata": "^0.1.13",
    "routing-controllers": "^0.9.0-alpha.6",
    "rss-finder": "^2.1.5",
    "rss-parser": "^3.10.0",
    "ts-node": "^9.1.1"
  },
  "devDependencies": {
    "@types/bcrypt": "^3.0.0",
    "@types/cookie-parser": "^1.4.2",
    "@types/express": "^4.17.11",
    "@types/jest": "^26.0.20",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/mongoose": "^5.10.3",
    "@typescript-eslint/eslint-plugin": "^4.14.0",
    "@typescript-eslint/parser": "^4.14.0",
    "eslint": "^7.18.0",
    "eslint-config-standard": "^16.0.2",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "jest": "^26.6.3",
    "ts-jest": "^26.4.4",
    "ts-node-dev": "^1.1.1",
    "typescript": "^4.1.3"
  }
